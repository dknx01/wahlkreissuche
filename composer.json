        "brick/geo": "^0.7.1",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.19",
        "gasparesganga/php-shapefile": "^3.4",
        "justinrainbow/json-schema": "^5.2",
        "league/csv": "^9.0",
        "longitude-one/doctrine-spatial": "^4.0",
        "matomo/matomo-php-tracker": "^3.1",
        "mjaschen/phpgeo": "^4.0",
        "mobiledetect/mobiledetectlib": "^4.8",
        "nelmio/cors-bundle": "^2.1",
        "phpdocumentor/reflection-docblock": "^5.3",
        "ramsey/collection": "^1.2",
        "ramsey/uuid": "^4.2",
        "ramsey/uuid-doctrine": "^1.8",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "^7.0",
        "symfony/cache": "~7.0.0",
        "symfony/console": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/dotenv": "^7.0",
        "symfony/expression-language": "^7.0",
        "symfony/flex": "^1.3.1",
        "symfony/form": "^7.0",
        "symfony/framework-bundle": "^7.0",
        "symfony/google-mailer": "~7.0.0",
        "symfony/http-client": "~7.0.0",
        "symfony/http-client-contracts": "^3.0.0",
        "symfony/http-foundation": "^7.0",
        "symfony/http-kernel": "^7.0",
        "symfony/intl": "^7.0",
        "symfony/mailer": "~7.0.0",
        "symfony/mime": "^7.0",
        "symfony/monolog-bundle": "^3.1",
        "symfony/process": "^7.0",
        "symfony/property-access": "^7.0",
        "symfony/property-info": "^7.0",
        "symfony/routing": "^7.0",
        "symfony/runtime": "*",
        "symfony/security-bundle": "~7.0.0",
        "symfony/security-csrf": "~7.0.0",
        "symfony/serializer": "^7.0",
        "symfony/stimulus-bundle": "^2.17",
        "symfony/string": "^7.0",
        "symfony/translation": "^7.0",
        "symfony/twig-bundle": "~7.0.0",
        "symfony/uid": "~7.0.0",
        "symfony/validator": "^7.0",
        "symfony/web-link": "^7.0",
        "symfony/web-profiler-bundle": "^7.0",
        "symfony/webpack-encore-bundle": "^2.0",
        "symfony/yaml": "^7.0",
        "symfonycasts/verify-email-bundle": "^1.10",
        "twig/extra-bundle": "^3.4",
        "twig/string-extra": "^3.4",
        "twig/twig": "^2.12 || ^3.0"
    },
    "require-dev": {
        "dbrekelmans/bdi": "^1.0",
        "ergebnis/composer-normalize": "^2.28",
        "fakerphp/faker": "^1.19",
        "friendsofphp/php-cs-fixer": "^3.5",
        "jangregor/phpstan-prophecy": "^1.0",
        "mikey179/vfsstream": "^1.6",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpstan/phpstan": "^1.4",
        "phpstan/phpstan-beberlei-assert": "^1.0",
        "phpstan/phpstan-doctrine": "^1.2",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/phpstan-symfony": "^1.1",
        "phpstan/phpstan-webmozart-assert": "^1.0",
        "phpunit/phpunit": "^10.0",
        "qossmic/deptrac": "^2.0",
        "roave/security-advisories": "dev-latest",
        "symfony/browser-kit": "^7.0",
        "symfony/css-selector": "^7.0",
        "symfony/debug-bundle": "^7.0",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/stopwatch": "^7.0",
        "symfony/var-dumper": "^7.0"
    },
    "replace": {
        "paragonie/random_compat": "^2.0",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\IntegrationTests\\": "tests/Integration",
            "App\\Tests\\": "tests/Common",
            "App\\UnitTests\\": "tests/Unit"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "ergebnis/composer-normalize": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "7.0.*"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "composer normalize",
            "./local-php-security-checker",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "./local-php-security-checker",
            "composer normalize",
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
